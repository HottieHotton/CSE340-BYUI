<!-- Content Heading -->
<% if (title) { %>
<h1 class="Login"><%= title %></h1>
<% } else { res.redirect('/') } %>

<!-- Message -->

<div id="messages">
  <% if (errors) { %>
  <ul class="notice">
    <% errors.array().forEach(error => { %> <% if(error.msg != "Invalid value")
    {%>
    <li><%= error.msg %></li>
    <% }}) %>
  </ul>
  <% } %> 
  <!-- <%= messages %> -->
</div>

<%= result %>